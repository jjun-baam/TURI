<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/img_top"
        android:layout_width="428dp"
        android:layout_height="84dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"

        app:srcCompat="@drawable/nemo"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imgb_ch1"
        android:layout_width="184dp"
        android:layout_height="231dp"
        android:layout_marginTop="16dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/imgb_ch3"
        app:layout_constraintEnd_toStartOf="@+id/imgb_ch2"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_top"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ch1"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imgb_ch2"
        android:layout_width="184dp"
        android:layout_height="231dp"
        android:layout_marginTop="16dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/imgb_ch4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_top"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ch2" />

    <ImageButton
        android:id="@+id/imgb_ch3"
        android:layout_width="184dp"
        android:layout_height="231dp"
        android:layout_marginTop="264dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imgb_ch4"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_top"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ch3" />

    <ImageButton
        android:id="@+id/imgb_ch4"
        android:layout_width="184dp"
        android:layout_height="231dp"
        android:layout_marginTop="264dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_top"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ch4" />

    <EditText
        android:id="@+id/ed_search"
        android:layout_width="280dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/search"
        android:ems="10"
        android:fontFamily="@font/noto_sans_bold"
        android:hint="지역 또는 장소명 입력"
        android:paddingLeft="50dp"
        android:inputType="text"
        android:maxLines="1"
        android:textColor="#9A9A9A"
        app:layout_constraintBottom_toBottomOf="@+id/img_top"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imgb_myt"
        android:layout_width="69dp"
        android:layout_height="69dp"
        android:layout_marginBottom="31dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.652"
        app:layout_constraintStart_toEndOf="@+id/ed_search"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.011"
        app:srcCompat="@drawable/myturi" />

    <ImageView
        android:id="@+id/img_board"
        android:layout_width="184dp"
        android:layout_height="61dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imgb_edit"
        app:layout_constraintHorizontal_bias="0.285"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgb_ch3"
        app:layout_constraintVertical_bias="0.164"
        app:srcCompat="@drawable/board" />

    <ImageView
        android:id="@+id/img_gongji"
        android:layout_width="184dp"
        android:layout_height="61dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toStartOf="@+id/imgb_edit"
        app:layout_constraintTop_toBottomOf="@+id/imgb_ch4"
        app:layout_constraintVertical_bias="0.164"
        app:srcCompat="@drawable/gongji" />

    <ImageButton
        android:id="@+id/imgb_edit"
        android:layout_width="69dp"
        android:layout_height="69dp"
        android:layout_marginTop="488dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_top"
        app:layout_constraintVertical_bias="0.209"
        app:srcCompat="@drawable/plus" />

</androidx.constraintlayout.widget.ConstraintLayout>